<!-- Form users_best start -->
<%= form_with(model: @users_best, local: true) do |f| %>
<!-- Form users_best end -->

  <!-- Label and input for dishes_id start -->
  <div class="form-group">
    <%= f.label :dishes_id %>

    <!-- Input for dishes_id start -->
    <%= f.text_field :dishes_id, class: "form-control" %>
    <!-- Input for dishes_id end -->
  </div>
  <!-- Label and input for dishes_id end -->

  <!-- Label and input for venue_id start -->
  <!-- Input for venue_id start -->
  <%= f.hidden_field :venue_id, value: @venue.id %>
  <!-- Input for venue_id end -->
  <!-- Label and input for venue_id end -->

  <!-- Label and input for user_id start -->
  <div class="form-group">
    <%= f.label :user_id %>

    <!-- Input for user_id start -->
    <%= f.text_field :user_id, class: "form-control" %>
    <!-- Input for user_id end -->
  </div>
  <!-- Label and input for user_id end -->

  <%= f.submit "Create users best", class: "btn btn-block btn-outline-secondary" %>
<% end %>
